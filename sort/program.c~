#include<stdio.h>
int bubble(int array){
    int temp;
    for (int j=0; j<sizeof(array); j++)
    for(int i=0; i<sizeof(array); i++){
        temp = array[i];
        if (temp > array[i+1]){
            array[i] = array[i+1];
            array[i+1] = temp;
        }
    }
}

int main(){
    int arr[100];
    arr = {1, 4, 3, 6, 7, 7, 2, 10}
    res = bubble(arr);
}
